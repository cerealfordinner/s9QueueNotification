{
    "manifest_version": 2,
    "name": "Square9 SalesForce Notifications",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["https://na156.lightning.force.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_pop": "popup.html"
    },
    "permissions": ["activeTab"],
    "web_accessible_resources": [
        "coin.wav"
    ]
}